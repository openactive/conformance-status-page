<header class="jumbotron">
  <div class="container">
    <h1>Conformance Status Page</h1>
    <p>Introduction text</p>
  </div>
</header>


<section class="container">
  <p>Main body section</p>
  <table class="table table-hover datasets-table">
    <thead>
    <tr>
      <th scope="col">col1</th>
      <th scope="col">col2</th>
      <th scope="col">col3</th>
    </tr>
    </thead>
    <tbody>
    {{#each statusData.publishers}}
      <tr>
        <td>{{@index}}</td><td>{{this.name}}</td><td>{{this.url}}</td>
      </tr>
    {{/each}}
    </tbody>
  </table>
</section>

{{#if error}}
<script>
  console.warn("Status source fetch: {{error}}");
</script>
{{/if}}
